abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/qdbus @{lib}/qt{5,6}/bin/qdbus
profile qdbus @{exec_path} {
  include <abstractions/base>
  include <abstractions/bus-session>

  @{exec_path} mr,

  include if exists <local/qdbus>
}

# vim:syntax=apparmor
