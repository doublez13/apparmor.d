abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/systemd-path
profile systemd-path @{exec_path} {
  include <abstractions/base>
  include <abstractions/consoles>
  include <abstractions/user-dirs>

  @{exec_path} mr,

  include if exists <local/systemd-path>
}

# vim:syntax=apparmor
